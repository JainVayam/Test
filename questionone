
function myFunction(s) {
    var a ,b,i,d;
    d=s.length;
    a= s[0];
    b=0;
    for(i=0; i<5;i++){
       if (s[i]>a){
       b=a;
      
       a=s[i];
       
       }
    }
    
    return b;
}

var points = [40, 100, 1, 5, 25, 10]; 
var c=myFunction(points);
document.getElementById("demo").innerHTML = c;
